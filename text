# Wait for password field
Wait Until Element Is Visible    xpath=//input[@name='credentials.passcode' or @id='input66']    timeout=20s

# Activate field
Click Element                    xpath=//input[@name='credentials.passcode' or @id='input66']
Sleep    1s

# Enter password
Input Text                       xpath=//input[@name='credentials.passcode' or @id='input66']    ${OKTA_PASS}
